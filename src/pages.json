{
  "easycom": {
    "autoscan": true,
    "custom": {
      "^Home(.*)": "@/pages/home/cps/Home$1.vue",
      "^uni-(.*)": "@dcloudio/uni-ui/lib/uni-$1/uni-$1.vue"
    }
  },
  "pages": [
    {
      "path": "pages/index/index",
      "style": {
        "navigationBarTitleText": "入口页"
      }
    },
    {
      "path": "pages/home/index",
      "style": {
        "onReachBottomDistance": 50,
        "navigationBarTitleText": "首页"
      }
    },
    {
      "path": "pages/findHouse/index",
      "style": {
        "navigationBarTitleText": "找房"
      }
    },
    {
      "path": "pages/friendCircle/index",
      "style": {
        "navigationBarTitleText": "房友圈"
      }
    },
    {
      "path": "pages/message/index",
      "style": {
        "navigationBarTitleText": "消息"
      }
    },
    {
      "path": "pages/profile/index",
      "style": {
        "navigationBarTitleText": "我的"
      }
    },
    {
      "path": "pages/search/index",
      "style": {
        "navigationBarTitleText": "搜索"
      }
    }
  ],
  "tabBar": {
    "custom": false,
    "color": "#7A7E83",
    "selectedColor": "#3cc51f",
    "borderStyle": "black",
    "backgroundColor": "#ffffff",
    "list": [
      {
        "pagePath": "pages/home/index",
        "text": "首页",
        "iconPath": "static/tabBar/home.png",
        "selectedIconPath": "static/tabBar/home_A.png"
      },
      {
        "pagePath": "pages/findHouse/index",
        "text": "找房",
        "iconPath": "static/tabBar/findHouse.png",
        "selectedIconPath": "static/tabBar/findHouse_A.png"
      },
      {
        "pagePath": "pages/friendCircle/index",
        "text": "房友圈",
        "iconPath": "static/tabBar/circlOfFriend.png",
        "selectedIconPath": "static/tabBar/circlOfFriend_A.png"
      },
      {
        "pagePath": "pages/message/index",
        "text": "消息",
        "iconPath": "static/tabBar/message.png",
        "selectedIconPath": "static/tabBar/message_A.png"
      },
      {
        "pagePath": "pages/profile/index",
        "text": "我的",
        "iconPath": "static/tabBar/myInfo.png",
        "selectedIconPath": "static/tabBar/myInfo_A.png"
      }
    ]
  },
  "globalStyle": {
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "成都购房通",
    "navigationBarBackgroundColor": "#F8F8F8",
    "backgroundColor": "#F8F8F8"
  }
}
